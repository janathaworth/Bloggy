<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
<cron>
<url>/cron/mycronjob</url>
<description>Daily 5pm CST email digest to all subscribers 
			 iff new posts in the last 24 hours. </description>
<schedule>every friday 17:00</schedule>
    <timezone>America/Central</timezone>
</cron>

<cron>
    <url>/retry</url>
    <description>Retry on jsdk</description>
    <schedule>every 10 minutes</schedule>
    <retry-parameters>
      <min-backoff-seconds>2.5</min-backoff-seconds>
      <max-doublings>5</max-doublings>
    </retry-parameters>
  </cron>
</cronentries>